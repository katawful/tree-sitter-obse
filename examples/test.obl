scn scriptname
begin blockmode
  if x
    eval x
  elseif x
    eval x
  else
    eval x
  endif
  Foreach item <- container[1]
    eval x
    break
    continue
  loop
  foreach item <- container["string"]
    eval x
  loop
  foreach item <- container[plain]
    eval x
  loop
  foreach item <- container[dot.object]
    eval x
  loop
end
